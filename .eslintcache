nfig,\n    );\n\n    alert('Valor depositado!');\n\n    history.push('/dashboard');\n  };\n\n  return (\n    <Container>\n      <Content>\n        <Form ref={formRef} onSubmit={handleSubmit}>\n          <h1>Digite o valor que deseja depositar</h1>\n\n          <Input name=\"amount\" placeholder=\"R$\" />\n\n          <Button type=\"submit\">Depositar</Button>\n        </Form>\n      </Content>\n    </Container>\n  );\n};\n\nexport default Deposit;\n","/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/hooks/auth.tsx",[],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/hooks/index.tsx",[],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/services/api.ts",[],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/utils/getValidationErros.ts",[],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/routes/index.tsx",[],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/routes/Route.tsx",[],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/pages/Dashboard/index.tsx",[],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/pages/Deposit/styles.ts",[],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/pages/BuyBitCoins/index.tsx",["130","131"],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/pages/SellBitCoins/index.tsx",["132","133"],"import React, { useRef } from 'react';\n\nimport { FormHandles } from '@unform/core';\nimport { useHistory } from 'react-router-dom';\n\nimport { Form } from '@unform/web';\nimport Input from '../../components/Input';\nimport api from '../../services/api';\n\nimport { Container, Content } from './styles';\nimport Button from '../../components/Button';\n\ninterface SellBitCoinsFormData {\n  bitCoinsAmount: string;\n}\n\nconst SellBitCoins: React.FC = () => {\n  const formRef = useRef<FormHandles>(null);\n  const history = useHistory();\n\n  const handleSubmit = async (data: SellBitCoinsFormData) => {\n    const token = localStorage.getItem('@GoBarber:token');\n    const config = {\n      headers: { Authorization: `Bearer ${token}` },\n    };\n\n    const bitCoinPrice = await api.get('btc/price', config);\n    const { sell } = bitCoinPrice.data;\n\n    const amountBRL = parseFloat(data.bitCoinsAmount) * sell;\n\n    try {\n      await api.post(\n        'btc/sell',\n        {\n          amount: amountBRL,\n        },\n        config,\n      );\n      alert(`Parabéns você vendeu ${data.bitCoinsAmount} bitcoins`);\n      history.push('/dashboard');\n    } catch {\n      alert('Não foi possível vender, pois você não possui essa quantidade');\n      history.push('dashboard');\n    }\n  };\n\n  return (\n    <Container>\n      <Content>\n        <Form ref={formRef} onSubmit={handleSubmit}>\n          <h1>Digite a quantidade de bitcoins que deseja vender</h1>\n\n          <Input name=\"bitCoinsAmount\" placeholder=\"Quantidade\" />\n\n          <Button type=\"submit\">Vender</Button>\n        </Form>\n      </Content>\n    </Container>\n  );\n};\n\nexport default SellBitCoins;\n","/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/pages/Extract/index.tsx",["134"],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/pages/Extract/styles.ts",[],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/pages/BuyBitCoins/styles.ts",[],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/components/Navbar/index.tsx",[],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/components/Navbar/styles.ts",[],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/components/Navbar/SidebarData.tsx",[],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/pages/SellBitCoins/styles.ts",[],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/pages/Dashboard/styles.tsx",[],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/components/Charts/LineChart.tsx",["135"],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/components/Charts/styles.ts",[],"/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/pages/SignUp/index.tsx",["136"],"import React, { useCallback, useRef } from 'react';\n\nimport { Form } from '@unform/web';\nimport * as Yup from 'yup';\nimport { FormHandles } from '@unform/core';\nimport { Link, useHistory } from 'react-router-dom';\n\nimport getValidationErrors from '../../utils/getValidationErros';\n\nimport Button from '../../components/Button';\nimport Input from '../../components/Input';\nimport { Container, Content } from './styles';\nimport api from '../../services/api';\n\ninterface SignUpFormData {\n  name: string;\n  email: string;\n  password: string;\n}\n\nconst SignUp: React.FC = () => {\n  const formRef = useRef<FormHandles>(null);\n  const history = useHistory();\n\n  const handleSubmit = useCallback(\n    async (data: SignUpFormData) => {\n      try {\n        formRef.current?.setErrors({});\n\n        const schema = Yup.object().shape({\n          name: Yup.string().required('Digite seu nome'),\n          email: Yup.string()\n            .email('Digite um e-mail válido')\n            .required('E-mail obrigatório'),\n          password: Yup.string().required('Senha obrigatória'),\n        });\n\n        await schema.validate(data, {\n          abortEarly: false,\n        });\n\n        api.post('/account', {\n          name: data.name,\n          email: data.email,\n          password: data.password,\n        });\n\n        alert('Você já pode fazer login!');\n\n        history.push('/');\n      } catch (err) {\n        if (err instanceof Yup.ValidationError) {\n          const errors = getValidationErrors(err);\n\n          formRef.current?.setErrors(errors);\n        }\n      }\n    },\n    [history],\n  );\n\n  return (\n    <Container>\n      <Content>\n        <Form ref={formRef} onSubmit={handleSubmit}>\n          <Input name=\"name\" placeholder=\"Nome\" />\n          <Input name=\"email\" placeholder=\"Email\" />\n          <Input name=\"password\" placeholder=\"Senha\" type=\"password\" />\n\n          <div className=\"grau\">\n            <Link to=\"/\">Retornar para login</Link>\n            <Button type=\"submit\">Entrar</Button>\n          </div>\n        </Form>\n      </Content>\n    </Container>\n  );\n};\n\nexport default SignUp;\n","/home/victor/desafio_eduzz/desafio-frontend-eduzz/src/pages/SignUp/styles.ts",[],{"ruleId":"137","severity":1,"message":"138","line":35,"column":5,"nodeType":"139","messageId":"140","endLine":35,"endColumn":31},{"ruleId":"137","severity":1,"message":"138","line":36,"column":7,"nodeType":"139","messageId":"140","endLine":36,"endColumn":77},{"ruleId":"137","severity":1,"message":"138","line":46,"column":7,"nodeType":"139","messageId":"140","endLine":46,"endColumn":69},{"ruleId":"137","severity":1,"message":"138","line":40,"column":7,"nodeType":"139","messageId":"140","endLine":40,"endColumn":68},{"ruleId":"137","severity":1,"message":"138","line":43,"column":7,"nodeType":"139","messageId":"140","endLine":43,"endColumn":77},{"ruleId":"141","severity":1,"message":"142","line":86,"column":7,"nodeType":"143","messageId":"140","endLine":86,"endColumn":18},{"ruleId":"144","severity":1,"message":"145","line":3,"column":10,"nodeType":"146","messageId":"147","endLine":3,"endColumn":25},{"ruleId":"137","severity":1,"message":"138","line":48,"column":9,"nodeType":"139","messageId":"140","endLine":48,"endColumn":43},"no-alert","Unexpected alert.","CallExpression","unexpected","no-console","Unexpected console statement.","MemberExpression","@typescript-eslint/no-unused-vars","'IoMdReturnRight' is defined but never used.","Identifier","unusedVar"]